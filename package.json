{
  "name": "gatsby-plugin-config",
  "version": "1.0.7",
  "description": "Gatsby config",
  "main": "lib/index.js",
  "repository": "https://github.com/birkir/gatsby-plugin-config",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "config",
    "dotenv",
    "environment variables",
    "env"
  ],
  "scripts": {
    "build": "babel src --out-dir ./lib --extensions '.ts'",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir ./lib"
  },
  "engines": {
    "node": ">=8.9"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-typescript": "7.1.0",
    "@babel/runtime": "7.1.2",
    "@types/node": "10.12.0",
    "cross-env": "5.2.0"
  }
}
